{
    "compilerOptions": {
        "target": "es6",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "inlineSourceMap": true,
        "inlineSources": true,
        "typeRoots": [
            "./node_modules/@types"
        ],
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceRoot": "./"
    },
    "include": [
        "./index.ts"
    ]
}
